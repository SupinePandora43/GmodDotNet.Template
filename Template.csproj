<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net5</TargetFramework>
		<PackageType>Template</PackageType>
		<PackageVersion>0.7.1</PackageVersion>
		<PackageId>Supine.GmodDotNetTemplate</PackageId>
		<Title>GmodDotNet Template</Title>
		<Authors>Supine</Authors>
		<Description>Templates to use when creating an application for Adatum Corporation.</Description>
		<PackageTags>dotnet-new;templates</PackageTags>
		<IncludeContentInPack>true</IncludeContentInPack>
		<IncludeBuildOutput>false</IncludeBuildOutput>
		<ContentTargetFolders>content</ContentTargetFolders>
	</PropertyGroup>
	<ItemGroup>
		<Content Include="TemplateModule/**" Exclude="TemplateModule\bin\**;TemplateModule\obj\**" />
		<Compile Remove="**/*" />
	</ItemGroup>
</Project>
